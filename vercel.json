{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "python3.11"
    }
  },
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "^/api/(.*)$", "dest": "api/index.py" },
    { "src": "^/health$", "dest": "api/index.py" },
    { "src": "^/$", "dest": "api/index.py" }
  ]
}
